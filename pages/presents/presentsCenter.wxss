/* pages/presents/presentsCenter.wxss */
page{
  background: #F3F3F3;
  width: 100%;
  height: 100%;
}
.page{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.filterTab-decoration{
  margin-top: 20rpx;
  background: white;
  z-index: 2;
}
.product-list{
  white-space: nowrap;
  overflow-y: scroll;
  flex: 1;
}
.search-page-decoration{
  z-index: 999;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}
.item-decorator{
  background: white;
  position: relative;
}
.item-decorator::after{
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background:rgba(224,224,224,1);
  height: 2rpx;
}
.item-decorator:last-of-type:after{
  content: none;
}