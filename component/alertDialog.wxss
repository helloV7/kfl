
.modal{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.45);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}
.dialog{
  width:555rpx;
  height:265rpx;
  background:rgba(255,255,255,1);
  border-radius:18rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.dialog-message{
  font-size:32rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  width: 100%;
  height: 144rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.dialog-control-layout{
  height: 120rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.dialog-cancel{
  display: inline-flex;
  justify-content: center;
  align-items: center;
  width: 376rpx;
  height: 120rpx;
  font-size:34rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.dialog-comfirm{
  display: inline-flex;
  justify-content: center;
  align-items: center;
  width: 376rpx;
  height: 120rpx;
  font-size:34rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(200,150,68,1);
}
.dialog-line-hor{
  width:477rpx;
  height:2rpx;  
  background:rgba(221,221,221,1);
}
.dialog-line-ver{
  width:1rpx;
  height:79rpx;
  background:rgba(221,221,221,1);
  display: inline-block;
}
